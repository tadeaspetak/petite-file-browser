{
  "name": "teleport-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "clean": "rm -rf ../my-app/node_modules & unlink yarn.lock",
    "dev": "yarn run dev:server & yarn run dev:client",
    "dev:client": "craco start",
    "dev:client:docker": "CHOKIDAR_USEPOLLING=true craco start",
    "dev:server": "nodemon server/index.ts",
    "dev:server:docker": "nodemon -L server/index.ts",
    "eject": "react-scripts eject",
    "start:server": "NODE_ENV=prod ts-node server/index.ts",
    "test": "craco test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.4.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nodemon": "^2.0.15",
    "postcss": "^7",
    "prettier": "^2.5.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  },
  "proxy": "http://localhost:3001"
}
